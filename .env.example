# Environment Configuration Template
# Copy this file to .env and update values

# MISP Configuration
API_KEY=your-misp-api-key-here
MISP_URL=https://misp.example.com
EVENT_ID=1234

# Output Files (paths on host machine)
OUTPUT_SRC=/var/www/html/misp-ip-src.txt
OUTPUT_DST=/var/www/html/misp-ip-dst.txt
OUTPUT_DOMAIN=/var/www/html/misp-domains.txt
OUTPUT_URL=/var/www/html/misp-urls.txt
OUTPUT_SURICATA=/var/www/html/misp-blocklist.rules

# SSL Verification (true/false)
VERIFY_SSL=false

# File Permissions
WWW_USER=www-data:www-data
